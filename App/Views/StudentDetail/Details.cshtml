}
<link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js">

<table class="table" style="width:70%">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AddmissionDate)
